<a class="btn btn-{{ app.user.hasVoted(request) ? '' : 'outline-' }}primary btn-sm mt-2"
   href="{{ get_vote_path(request) }}"
   data-bs-toggle="tooltip"
   data-bs-placement="left"
   data-bs-html="true"
   title="<div class='text-white'><ul>{% for vote in request.votes %}<li>{{ vote.voter.fullName }}</li>{% endfor %}</ul></div>"
    >
    <i class="fa fa-thumbs-up"></i>
    <span>({{ request.votes|length }})</span>
</a>
